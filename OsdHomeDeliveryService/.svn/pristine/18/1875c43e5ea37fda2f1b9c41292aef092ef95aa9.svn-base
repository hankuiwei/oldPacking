<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <TableLayout
        android:id="@+id/tableParents"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/layout_25dp"
        android:layout_marginLeft="@dimen/layout_25dp"
        android:layout_marginRight="@dimen/layout_25dp"
        android:background="@color/gray_cccccc"
        android:orientation="vertical">

        <TableRow
            android:id="@+id/tableRowName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/gray_F8F8F8"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/text_product_sn"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <TextView
                android:id="@+id/text_productSN"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="3"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRowTel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/white"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/text_product_name"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <TextView
                android:id="@+id/text_productName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="3"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRowAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/gray_F8F8F8"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingBottom="@dimen/layout_13dp"
                android:paddingTop="@dimen/layout_13dp"
                android:text="@string/text_product_createtime"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <LinearLayout
                android:id="@+id/linear_customerAdd"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_weight="3"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/layout_13dp"
                android:paddingTop="@dimen/layout_13dp">


                <TextView
                    android:id="@+id/text_productCreateTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="7"
                    android:textColor="#666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:id="@+id/tableRowProduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/white"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingBottom="@dimen/layout_13dp"
                android:paddingTop="@dimen/layout_13dp"
                android:text="@string/text_bout_time"
                android:textColor="@color/gray_666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <TextView
                android:id="@+id/text_productBoutTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_weight="3"
                android:paddingBottom="@dimen/layout_13dp"
                android:paddingTop="@dimen/layout_13dp"
                android:textColor="@color/gray_666666"
                android:textSize="@dimen/text_15sp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRepairDeadLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/gray_F8F8F8"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/text_ensurence_endtime"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <TextView
                android:id="@+id/text_ensurenceEndline"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="3"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px"
            android:background="@color/white_ffffff"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/text_getdoor_endtime"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />

            <LinearLayout
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/gray_cccccc" />

            <TextView
                android:id="@+id/text_getDoorEndLine"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/layout_13dp"
                android:layout_marginLeft="@dimen/layout_10dp"
                android:layout_marginStart="@dimen/layout_10dp"
                android:layout_marginTop="@dimen/layout_13dp"
                android:layout_weight="3"
                android:textColor="#666666"
                android:textSize="@dimen/text_15sp" />
        </TableRow>
    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_20dp"
        android:background="@color/gray_F8F8F8"></LinearLayout>
</LinearLayout>