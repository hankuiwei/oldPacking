<?xml version="1.0" encoding="utf-8"?>
<resources>
    

    <declare-styleable name="HomeTabView">
        <attr name="icon_size" format="dimension"></attr>
        <attr name="text_size" format="dimension"></attr>
        <attr name="text_select_color" format="color"></attr>
        <attr name="text_normal_color" format="color"></attr>
        <attr name="item_padding" format="dimension"></attr>
    </declare-styleable>


    <declare-styleable name="roundedimageview">

        <attr
            name="border_thickness"
            format="dimension"
            />

        <attr
            name="border_inside_color"
            format="color"
            />

        <attr
            name="border_outside_color"
            format="color"/>

    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>
