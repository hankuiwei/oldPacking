<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:background="@color/black_373b3e"
    tools:context="com.lenovo.csd.eservice.activity.BaseActivity">
    <LinearLayout
        android:id="@+id/notChangeBack"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/layout_15dp"
        android:paddingBottom="@dimen/layout_25dp"
        android:background="#373B3E"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="@dimen/layout_15dp"
            android:layout_height="@dimen/layout_25dp"
            android:src="@drawable/icon_back"
            android:layout_marginLeft="@dimen/layout_25dp"
            android:layout_marginStart="@dimen/layout_25dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingEnd="0dp"
            android:paddingLeft="@dimen/layout_10dp"
            android:paddingRight="0dp"
            android:paddingStart="@dimen/layout_10dp"
            android:text="@string/add_notchange"
            android:textColor="@color/white"
            android:textSize="@dimen/text_23sp" />
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="@dimen/layout_25dp"
        android:paddingStart="@dimen/layout_25dp"
        android:paddingRight="@dimen/layout_25dp"
        android:paddingEnd="@dimen/layout_25dp">
        <TextView
            android:layout_marginTop="@dimen/layout_30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/notchange_kinds"
            android:textSize="@dimen/text_24sp"
            android:textColor="@color/gray_666666"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/layout_20dp"
            android:orientation="vertical"
            android:background="@drawable/shape_4dpcorner_white_bg">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/layout_10dp"
                android:paddingStart="@dimen/layout_10dp"
                android:paddingRight="@dimen/layout_10dp"
                android:paddingEnd="@dimen/layout_10dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/layout_20dp"
                    android:layout_marginBottom="@dimen/layout_20dp"
                    android:text="@string/componentClass"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_18sp"
                    android:textColor="@color/gray_666666" />
                <Spinner
                    android:id="@+id/Spin_componentClass"
                    android:layout_marginLeft="@dimen/layout_40dp"
                    android:layout_marginStart= "@dimen/layout_40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    style="@android:style/Widget.Spinner.DropDown"
                    />
                <ImageView
                    android:layout_width="@dimen/layout_10dp"
                    android:layout_height="@dimen/layout_15dp"
                    android:layout_marginTop="@dimen/layout_20dp"
                    android:layout_marginBottom="@dimen/layout_20dp"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/icon_foreward" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_1dp"
                android:background="@color/gray_cccccc"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/layout_10dp"
                android:paddingStart="@dimen/layout_10dp"
                android:paddingRight="@dimen/layout_10dp"
                android:paddingEnd="@dimen/layout_10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nonChangingParts"
                    android:layout_marginBottom="@dimen/layout_20dp"
                    android:layout_marginTop="@dimen/layout_20dp"
                    android:textSize="@dimen/text_18sp"
                    android:textColor="@color/gray_666666"
                    android:gravity="center_vertical"/>
                <Spinner
                    android:id="@+id/Spin_nonChangingParts"
                    android:layout_marginLeft="@dimen/layout_20dp"
                    android:layout_marginStart="@dimen/layout_20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="@null"
                    style="@android:style/Widget.Spinner.DropDown"
                    />
                <ImageView
                    android:layout_width="@dimen/layout_10dp"
                    android:layout_height="@dimen/layout_15dp"
                    android:layout_marginBottom="@dimen/layout_20dp"
                    android:layout_marginTop="@dimen/layout_20dp"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/icon_foreward" />
            </LinearLayout>
        </LinearLayout>


            <Button
                android:id="@+id/Btn_AddNotChangeCommit"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_70dp"
                android:layout_marginTop="@dimen/layout_30dp"
                android:text="@string/text_commit"
                android:textColor="@color/white"
                android:textSize="@dimen/text_24sp"
                android:background="@drawable/selector_bg_redbtn"/>


    </LinearLayout>
    </ScrollView>
</LinearLayout>
