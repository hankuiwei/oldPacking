<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/Lin_ChangeGroup"
    android:background="@drawable/selector_bg_change_me"
    android:orientation="vertical"
    android:descendantFocusability="blocksDescendants"
    android:paddingTop="@dimen/layout_30dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/sparePart_id"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_15sp" />

                <TextView
                    android:id="@+id/Txt_sparePartId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginStart="@dimen/layout_10dp"
                    android:textIsSelectable="true"
                    android:textColor="@color/gray_666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/layout_7dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/sparePart_bar"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_15sp" />

                <TextView
                    android:id="@+id/Txt_sparePartBar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginStart="@dimen/layout_10dp"
                    android:textColor="@color/gray_666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/layout_7dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/sparePart_describe"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_15sp" />

                <TextView
                    android:id="@+id/Txt_SparePartDescribe"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginStart="@dimen/layout_10dp"
                    android:textColor="@color/gray_666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/layout_7dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/sparePart_classification"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_15sp" />

                <TextView
                    android:id="@+id/Txt_sparePartClassificat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginStart="@dimen/layout_10dp"
                    android:textColor="@color/gray_666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/layout_7dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/sparePart_price_up"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_15sp" />

                <TextView
                    android:id="@+id/Txt_sparePartPriceUp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginStart="@dimen/layout_10dp"
                    android:textColor="@color/gray_666666"
                    android:textSize="@dimen/text_15sp" />
            </LinearLayout>
        </LinearLayout>

        <CheckBox
            android:id="@+id/RadioBtn_select"
            android:layout_width="@dimen/layout_20dp"
            android:layout_height="@dimen/layout_20dp"
            android:focusable="false"
            android:clickable="false"
            android:button = "@null"
            android:checked="true"
            android:background="@drawable/selector_radio_change"
            android:layout_gravity="center_vertical" />
        <!--
        android:button="@drawable/selector_radio_change"
        -->
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Lin_changeLine"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="@dimen/layout_30dp"
        android:background="@color/gray_cccccc"
        android:orientation="horizontal" />
</LinearLayout>